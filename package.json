{
  "name": "crawlee-py",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:api": "bun run --watch --hot api/index.ts",
    "build:api": "bun build api/index.ts --outdir dist/api --target bun",
    "start:api": "bun run build:api && bun run dist/api/index.js",
    "build:mcp": "bun build mcp-server/index.ts --outdir dist/mcp-server --target node",
    "start:mcp": "bun run build:mcp && node ./dist/mcp-server/index.js",
    "test:mcp": "bun test ./mcp-server/tests/**"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "devDependencies": {
    "@prisma/client": "^6.5.0",
    "@types/bun": "latest",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "prisma": "^6.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@valibot/to-json-schema": "^1.0.0",
    "arktype": "^2.1.16",
    "effect": "^3.14.6",
    "express": "^5.1.0",
    "fastmcp": "^1.21.0",
    "hono": "^4.7.5",
    "lodash": "^4.17.21",
    "zod": "^3.24.2"
  }
}
